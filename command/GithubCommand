## git push 

$ git add <file_name>
$ git commit -m "<msg>"
$ git push origin <branch_name>


##create a new repository on the command line

echo "# test" >> readme.md

git init

git add readme.md

git commit -m "first commit"

git branch -m main

git remote add origin https://github.com/rickz42/<repositoryname.git>

git push -u origin main

#If you want to remove large file:
git filter-branch -f --index-filter 'git rm -r --cached --ignore-unmatch <filename>' HEAD